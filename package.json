{
  "name": "rpaster-vscode",
  "displayName": "rpaster-vscode",
  "description": "Integrate the power of RPaste with VSCode!",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:rpaster-vscode.upload"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rpaster-vscode.upload",
        "title": "Upload file content to rpaste"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "copy-paste": "^1.3.0"
  }
}
